<p style="color: green"><%= notice %></p>

<%= render @foto %>

<% @foto.images.each do |image| %>
  <%= image_tag image %>
<% end %
<div>
  <%= link_to "Edit this foto", edit_foto_path(@foto) %> |
  <%= link_to "Back to fotos", fotos_path %>

  <%= button_to "Destroy this foto", @foto, method: :delete %>
</div>
